{
  "name": "changelog",
  "type": "module",
  "version": "1.0.0",
  "description": "generate changelog form git tags and commits for github",
  "author": "",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "docs:all": "npm run docs:clean && npm run docs:html && npm run docs:md && npm run docs:table",
    "docs:clean": "rimraf docs-html docs-md docs",
    "docs:html": "typedoc --options typedoc.json",
    "docs:md": "typedoc --options typedoc.md.json",
    "docs:table": "typedoc --options typedoc.json"
  },
  "dependencies": {
    "cli-progress": "^3.12.0",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/semver": "^7.7.1",
    "typedoc": "^0.28.12",
    "typedoc-plugin-markdown": "^4.8.1"
  }
}
