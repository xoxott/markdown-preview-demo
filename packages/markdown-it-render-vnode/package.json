{
  "name": "@suga/markdown-it-render-vnode",
  "version": "1.4.0",
  "description": "Markdown-it plugin for rendering markdown to framework-agnostic virtual nodes",
  "author": {
    "name": "yangtao",
    "email": "212920320@qq.com"
  },
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./adapters": {
      "types": "./src/adapters/types.ts",
      "default": "./src/adapters/types.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": ["./src/*"]
    }
  },
  "keywords": [
    "markdown",
    "markdown-it",
    "framework",
    "vnode",
    "plugin",
    "renderer",
    "vue",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xoxott/suga-tools.git",
    "directory": "packages/markdown-it-render-vnode"
  },
  "homepage": "https://github.com/xoxott/suga-tools/tree/main/packages/markdown-it-render-vnode",
  "bugs": {
    "url": "https://github.com/xoxott/suga-tools/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "markdown-it": "^14.0.0"
  },
  "peerDependenciesMeta": {
    "markdown-it": {
      "optional": false
    }
  },
  "devDependencies": {
    "@suga/markdown-it-render-vnode-vue": "workspace:*",
    "@types/markdown-it": "^14.1.2",
    "@types/uuid": "^11.0.0",
    "typescript": "^5.6.3",
    "vue": "^3.5.13",
    "vitest": "^4.0.8"
  }
}

